{
    "version": 2,
    "alias" : "financialUI",
    "name": "financial_portal",
    "builds": [
      {"src": "package.json", "use": "@now/static-build"}
    ],
    "routes": [
      {"src": "^/js/(.*)", "dest": "/js/$1"},
      {"src": "^/css/(.*)", "dest": "/css/$1"},
      {"src": "^/img/(.*)", "dest": "/img/$1"},
      {"src": "^/public/(.*)","dest": "/public/$1"},
      {"src": "^/favicon.ico$", "dest": "/favicon.ico"},
      {"src": ".*", "dest": "/index.html"}
    ]
  }